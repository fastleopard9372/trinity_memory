// .env.example
# Server
NODE_ENV=development
PORT=3000

# Database (Supabase)
DATABASE_URL="postgresql://postgres:[PASSWORD]@[HOST]:[PORT]/postgres"
SUPABASE_URL="https://[PROJECT_ID].supabase.co"
SUPABASE_ANON_KEY="your-anon-key"
SUPABASE_SERVICE_KEY="your-service-key"

# Pinecone
PINECONE_API_KEY="your-pinecone-api-key"
PINECONE_ENV="your-pinecone-environment"
PINECONE_INDEX="trinity-memory"

# OpenAI
OPENAI_API_KEY="your-openai-api-key"

# NAS Configuration
NAS_TYPE="webdav" # Options: webdav, smb, rclone
NAS_URL="https://your-nas-url/webdav"
NAS_USERNAME="your-nas-username"
NAS_PASSWORD="your-nas-password"
NAS_MOUNT_PATH="/trinity"

# n8n Webhook
N8N_WEBHOOK_URL="https://your-n8n-instance/webhook/trinity"

# Logging
LOG_LEVEL="info"